<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.gb.storage.client.Controller.MainSceneController">
   <children>
      <VBox fx:id="AuthBox" layoutX="281.0" layoutY="248.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextField fx:id="loginField" promptText="login">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin></TextField>
            <PasswordField fx:id="passwordField" promptText="password">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></PasswordField>
            <Button mnemonicParsing="false" onAction="#sendAuth" text="Log in">
               <VBox.margin>
                  <Insets left="75.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
